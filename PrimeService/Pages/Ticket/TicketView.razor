@page "/Ticket"

@using FC.PrimeService.Shopping.Shop.Component
@using FC.PrimeService.Tickets.Ticket

@switch (_viewId)
{
    case "Ticket":
        <TicketComponent /> 
        @* <MudText>@_viewId</MudText> *@
        @* <MudText>@_id</MudText> *@
        break;
    case "POList":
        <POSComponent Id="@_id" />
        break;
    default:
        _navigationManager.NavigateTo("/");
        break;
}

<MudPaper Elevation="3" Class="mt-2 d-flex justify-end">
    <MudGrid>
        <MudItem xs="12" sm="6" md="6">
            <MudBreadcrumbs Items="_items"></MudBreadcrumbs>
        </MudItem>
        <MudItem xs="12" sm="6" md="6">
            
        </MudItem>
    </MudGrid>
</MudPaper>

<MudScrollToTop>
    <MudButton Variant="Variant.Filled" StartIcon="@Icons.Filled.ArrowUpward" Color="Color.Secondary">Go to top</MudButton>
</MudScrollToTop>

