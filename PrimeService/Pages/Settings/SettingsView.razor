@page "/SettingsView"
@using FC.PrimeService.Common.Settings
@using FC.PrimeService.Common.Settings.ListItems

<MudBreadcrumbs Items="_items"></MudBreadcrumbs>

@switch (_viewId)
{
    case "Company":
        <CompanyUx />
        break;
    case "Location":
        <WorkLocationList />
        break;
    case "Employee":
        <EmployeeList />
        break;
    case "Status":
        <StatusList />
        break;
    case "Services":
        <ServiceTypeList />
        break;
    case  "Account":
        <PaymentTagList />
        break;
    case  "PaymentMethod":
        <PaymentMethodsList />
        break;
    case "Client-Type":
        <ClientTypeList />
        break;
    case "PCategory":
        <ProductCategoryList />
        break;
    default:
        _navigationManager.NavigateTo("/ComingSoon");
        break;
}