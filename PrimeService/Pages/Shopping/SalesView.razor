@page "/Sales"

@using FC.PrimeService.Shopping.Shop.Component
@using FC.PrimeService.Shopping.Shop.ListItems

<MudBreadcrumbs Items="_items"></MudBreadcrumbs>

@switch (_viewId)
{
    case "POS":
        <ShopList />
        break;
    case "POList":
        <POSComponent Id="@_id" />
        break;
    default:
        _navigationManager.NavigateTo("/");
        break;
}